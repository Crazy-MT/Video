<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taguxdesign.maotong.myvideo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DesignDemo">

        <activity android:name=".movie.MovieFragment">

        </activity>
        <activity android:name=".VideoDetailActivity"></activity>

        <activity android:name=".cheesesquare.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".cheesesquare.CheeseDetailActivity"
            android:parentActivityName=".cheesesquare.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".cheesesquare.MainActivity" />
        </activity>
    </application>

</manifest>